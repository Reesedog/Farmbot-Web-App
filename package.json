    "@types/markdown-it": "12.2.1",
    "@types/node": "16.9.4",
    "@types/promise-timeout": "1.3.0",
    "@types/react": "17.0.22",
    "@types/react-color": "3.0.5",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/ws": "7.4.7",
    "axios": "0.21.4",
    "bowser": "2.11.0",
    "boxed_value": "1.0.0",
