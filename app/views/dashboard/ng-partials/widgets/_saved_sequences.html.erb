<div class="row">
  <div class="small-12 columns header-wrapper">
    <h5>Saved Sequences</h5>
  </div>
</div>

<div class="row">
  <div class="content-wrapper">
    <button
      class="full-width margin green button-like text-left"
      ng-click="addSequence()">
      <i class="step fi-plus size-12"></i>
      New Sequence
    </button>
    <div ng-repeat="seq in storedSequences track by $id(seq)">
      <button
        ng-click="load(seq)"
        class="full-width text-left margin no-radius {{seq.color}}-block">
        {{seq.name}}<i class="fa fa-bars right"></i>
      </button>
    </div>
  </div>
</div>
