.row
  .large-4.small-12.columns
    .row
      .large-12.small-12.columns
        .header-wrapper
          %h5 Move Relative
      .large-12.small-12.columns
        .content-wrapper
          %h6 Move Amount(mm)
          .row
            %button.x-small.button.radius
              Manual
            %button.x-small.button.radius{ng_click: 'setMultiplier(1)'}
              1
            %button.x-small.button.radius{ng_click: 'setMultiplier(10)'}
              10
            %button.x-small.button.radius{ng_click: 'setMultiplier(100)'}
              100
            %button.x-small.button.radius{ng_click: 'setMultiplier(1000)'}
              1000
          .row
            / sorry. #shipit
            %table{style:'border: transparent;'}
              %tr
                %td
                %td
                  %directionbutton{direction: "up", axis: "y"}
                %td
                %td
                %td
                  %directionbutton{direction: "up", axis: "z"}
              %tr{style: 'background-color: transparent !important;'}
                %td
                  %directionbutton{direction: "up", axis: "x"}
                %td
                  %directionbutton{direction: "down", axis: "y"}
                %td
                  %directionbutton{direction: "down", axis: "x"}
                %td
                %td
                  %directionbutton{direction: "down", axis: "z"}
          .row
            .large-12.columns
              %button.xx-small.button.radius.stop.expand STOP
      .large-12.small-12.columns
        .header-wrapper
          %h5 Tool Control
      .large-12.small-12.columns
        .content-wrapper
          .row
            .large-6.small-6.columns
              %label.left.inline Vacuum Pump
            .large-6.small-6.columns
              .switch.small.radius
                %input#exampleRadioSwitch2{:name => "testGroup", :type => "radio"}/
                %label{:for => "exampleRadioSwitch2"}
          .row
            .large-6.small-6.columns
              %label.left.inline Water Pump
            .large-6.small-6.columns
              .switch.small.radius
                %input#exampleRadioSwitch2{:name => "testGroup", :type => "radio"}/
                %label{:for => "exampleRadioSwitch2"}
          .row
            .large-6.small-6.columns
              %label.left.inline LED
            .large-6.small-6.columns
              .switch.small.radius
                %input#exampleRadioSwitch2{:name => "testGroup", :type => "radio"}/
                %label{:for => "exampleRadioSwitch2"}
  .large-4.small-12.columns
    .row
      .large-12.small-12.columns
        .header-wrapper
          %h5 Move Absolute
      .large-12.small-12.columns
        .content-wrapper
          .row
            .large-4.small-4.columns
              %label.left.inline.shrink Gantry(X)
            .large-4.small-4.columns
              %input{:placeholder => "3.124m", :type => "text"}/
            .large-4.small-4.columns
              %button.xx-small.expand.button.radius HOME X
          .row
            .large-4.small-4.columns
              %label.left.inline.shrink Cross Slide(Y)
            .large-4.small-4.columns
              %input{:placeholder => "1.210m", :type => "text"}/
            .large-4.small-4.columns
              %button.xx-small.expand.button.radius HOME Y
          .row
            .large-4.small-4.columns
              %label.left.inline.shrink Tool Mount(Z)
            .large-4.small-4.columns
              %input{:placeholder => "0.893m", :type => "text"}/
            .large-4.small-4.columns
              %button.xx-small.expand.button.radius HOME Z
          .row
            .large-4.small-4.columns
              %button.xx-small.expand.button.radius.stop STOP
            .large-4.small-4.columns
              %button.xx-small.expand.button.radius.go GO
            .large-4.small-4.columns
              %button.xx-small.expand.button.radius HOME ALL
    .row
      .large-12.small-12.columns
        .header-wrapper
          %h5 Camera 1
      .large-12.small-12.columns
        %img{src: "http://108.90.200.9:8080/?action=stream", style: "width: 1080px; height: auto;"}

  .large-4.small-12.columns
    .row
      .large-12.small-12.columns
        .header-wrapper
          %h5 Device Status
      .large-12.small-12.columns
        .content-wrapper
          .row
            .large-6.small-6.columns
              %label.left.inline Current Operation
            .large-6.small-6.columns
              %label.left.inline Idle
          .row
            .large-6.small-6.columns
              %label.left.inline Next Operation
            .large-6.small-6.columns
              %label.left.inline Current Operation
          .row
            .large-6.small-6.columns
              %label.left.inline Gantry Position(X)
            .large-6.small-6.columns
              %label.left.inline 3.124 m
          .row
            .large-6.small-6.columns
              %label.left.inline Cross Slide Position(Y)
            .large-6.small-6.columns
              %label.left.inline 1.210 m
          .row
            .large-6.small-6.columns
              %label.left.inline Z-Axis Position(Z)
            .large-6.small-6.columns
              %label.left.inline 0.893 m
          .row
            .large-6.small-6.columns
              %button.xx-small.expand.button.radius.stop STOP
            .large-6.small-6.columns
              %button.xx-small.expand.button.radius.restart RESTART
